<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@Page.Title</title>
    <script type="text/javascript" src="http://use.typekit.com/yxo3unq.js"></script>
    @HTML.jQuery(false)
    @HTML.CSS("screen.css")
    @HTML.CSS("print.css", "print")
    @HTML.CSS("screen.css")
    @HTML.CSS("plugins/fancy-type/screen.css")
    @HTML.Script("jquery.cycle.all.js")
    <!--[if lt IE 8]><link rel="stylesheet" href="/public/stylesheets/ie.css" type="text/css" media="screen, projection"><![endif]-->
</head>
<body>
    <div class="container">
        <div class="span-16 prepend-6">
            @HTML.ImageLink(URL.SiteRoot(), "publishedLogo.bmp")
        </div>
        <hr>
        <hr class="space">

        @RenderBody()

        <!--footer---->
        <hr>
      
        <p>
            &copy;@DateTime.Now.Year Published, Inc.
            @Snippets.FooterText();
        </p>

        <hr>
      
    </div>
</body>
</html>
